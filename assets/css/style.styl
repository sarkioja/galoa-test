//Reset
@require _normalize
@import _reset
@import _typography

// Main Colors
$blue = #009ade
$pink = #ec0080
$yellow = #ffea00
$gray = #4c4c4c
$black = #000
$white = #fff

// Styles
body
  background-color $white
  color $black
  font-family 'CNLatin', sans-serif
  font-size 2em


h1
  text-transform uppercase

h2
  font-family 'Whitney', sans-serif
  font-weight 100
  font-size 1.5em
